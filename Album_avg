stoney = [70, 75, 80, 88, 68, 100, 90, 88, 94, 65, 83, 100, 80, 95,  79, 84, 87, 93]
avg = sum(stoney)/len(stoney)
print("The album score of Stoney is", round(avg, 2))

beerbongs_bentleys = [88, 91, 100, 86,  80, 92, 90,  100, 100, 89, 81, 85, 91,  84, 80, 81,  84, 82]
avg_2 = sum(beerbongs_bentleys)/len(beerbongs_bentleys)
print("\nThe album score of Beerbongs & Bentleys is", round(avg_2, 2))

hollywoods_bleeding =  [86, 100, 85, 87, 84, 91, 93, 85, 94, 100, 88, 100, 85, 100, 84, 80, 95]
avg_3 = sum(hollywoods_bleeding)/len(hollywoods_bleeding)
print("\nThe album score of Hollywood's Bleeding is", round(avg_3, 2))

twelve_carat_toothache =  [86, 100, 84, 96, 90, 91, 85, 88, 92, 83, 85, 80, 100, 80, 92, 92]
avg_4 = sum(twelve_carat_toothache)/len(twelve_carat_toothache)
print("\nThe album score of Twelve Carat Toothache is", round(avg_4, 2))

avg_5 = sum(stoney + beerbongs_bentleys + hollywoods_bleeding + twelve_carat_toothache)/len(stoney + beerbongs_bentleys + hollywoods_bleeding + twelve_carat_toothache)
print("\nPosty's overall rating is", round(avg_5, 2))
